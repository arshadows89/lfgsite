<%= form_for :guild, url: guilds_path, method: "post", :html => { :class => 'form-horizontal', multipart: true }  do |f| %>
	<div id="guildformtext">
    Before we start fill out some information about your charachter.
  </div>
	<%= f.text_field :battletag, placeholder: "BattleTag" %><br />
  <%= f.text_field :email, placeholder: "Email" %><br />
  <%= f.text_field :charname, placeholder: "Charachter Name" %><br />
  <%= f.text_field :armory, placeholder: "Charachter Armory" %><br />
  <%= f.select :charclass, [['Pick a Class'], ['Death Knight'], ['Druid'], ['Hunter'], ['Mage'], ['Monk'], ['Paladin'], ['Priest'], ['Rogue'], ['Shaman'], ['Warlock'], ['Warrior']] %><br />
<!--   <%= f.select :role, [['Pick a Role'], ['Dps'], ['Healer'], ['Tank']] %><br /> -->
  <div class='charclass'>
     <%= f.select :spec, [['Pick a Spec']], :class => 'charclass', :id => 'Pick a Class' %><br />
  </div>

  <%= f.text_field :level, placeholder: "Level" %><br />
	<%= f.button "Back", :class => 'guildbuttonback' %>
	<%= f.submit "Submit", :class => 'guildsubmit' %>
<% end %>

